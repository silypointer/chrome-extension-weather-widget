{
    "manifest_version": 3,
    "name": "Weather Widget",
    "description": "Weather Widget By Rohan Patil",
    "version": "0.2",
    "icons": {
      "16": "images/weather_16.png",
      "128": "images/weather_128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.5.1.min.js", "getWeather.js"]
        }
      ],
      "action": {
        "default_popup": "popup.html"
      },
      "permissions": [
        "activeTab",
        "scripting"
      ]
  }